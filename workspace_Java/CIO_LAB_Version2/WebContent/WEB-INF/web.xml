<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CIO_LAB</display-name>
  <welcome-file-list>
    <welcome-file>Test.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>Verify_Login.Verify_User</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>testX</servlet-name>
    <servlet-class>Verify_Login.View_Devices</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>testM</servlet-name>
    <servlet-class>Verify_Login.FileUploadHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>testF</servlet-name>
    <servlet-class>Verify_Login.FTE_SCAN</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/Details.CIO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testX</servlet-name>
    <url-pattern>/View.CIO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testM</servlet-name>
    <url-pattern>/FLASH.CIO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testF</servlet-name>
    <url-pattern>/FTE_SCAN.CIO</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>
      org.apache.commons.fileupload.servlet.FileCleanerCleanup
    </listener-class>
  </listener>
</web-app>